{
  "name": "SUSI Extension",
  "version": "1.0",
  "description": "Makes the lists in SUSI more readable.",
  "manifest_version": 2,
  "permissions": ["activeTab", "declarativeContent", "http://*/", "https://*/"],
  "content_scripts": [
    {
      "matches": ["*://susi4.uni-sofia.bg/*"],
      "css": ["susi.css"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "SUSI Extension",
    "default_popup": "popup.html"
  }
}
